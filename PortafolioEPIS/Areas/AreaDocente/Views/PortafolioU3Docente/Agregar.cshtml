                                var aprobados = document.getElementById("aprobados").value;
                                var unidad = "UIII";
                                var stado = "G";
                                window.location.href = '@Url.Content("~/AreaDocente/PortafolioU3Docente/Guardar/")' + "?idprueba=" + idprueba + "&retirados=" + retirados + "&abandonos=" + abandonos + "&aprobados=" + aprobados + "&codigo=" + @Model.Codigo_DetalleCargaAcademica + "&estado=" + stado + "&unidad=" + unidad;
                            }
                        </script>
                    </div>
