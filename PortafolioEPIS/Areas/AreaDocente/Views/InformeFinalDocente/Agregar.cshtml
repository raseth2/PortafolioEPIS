                        <script>
                            //Función que realiza la suma
                            function Suma() {
                                var ingreso1 = document.calculadora.estudiantesabandono.value;
                                var ingreso2 = document.calculadora.estudiantesretiro.value;
                                var ingreso3 = document.calculadora.matriculados.value;
                                try {
                                    //Calculamos el número escrito:
                                    ingreso1 = (isNaN(parseInt(ingreso1))) ? 0 : parseInt(ingreso1);
                                    ingreso2 = (isNaN(parseInt(ingreso2))) ? 0 : parseInt(ingreso2);
                                    document.calculadora.estudiantesasistentes.value = ingreso3 - (ingreso1 + ingreso2);
                                }
                                //Si se produce un error no hacemos nada
                                catch (e) { }
                            }
                            function calaprobados() {
                                var estaprobados = document.calculadora.estudiantesaprobados.value;
                                var estasistentes = document.calculadora.estudiantesasistentes.value;
                                try {
                                    estaprobados = (isNaN(parseInt(estaprobados))) ? 0 : parseInt(estaprobados);
                                    estasistentes = (isNaN(parseInt(estasistentes))) ? 0 : parseInt(estasistentes);
                                    document.calculadora.estudiantesdesaprobados.value = estasistentes - estaprobados;
                                }
                                catch (e) { }

                            }
                        </script>




