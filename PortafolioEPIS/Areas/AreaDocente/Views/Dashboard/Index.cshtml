<!--Resumen general de CARGA ACDEMICA-->
<div class="card-box ">
    <div class="row clearfix">
        <div class="col-md-12">
            <center><h2 class="text-primary">Información de Carga Académica 2019-I</h2></center>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-12">
            <table id="s" class="table table-hover">
                <thead>
                    <tr>
                        <th>Docente</th>
                        <th>#</th>
                        <th>Curso</th>
                        <th>Horas</th>
                        <th>Creditos</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var p in detalle)
                    {
                        if (p.Codigo_Docente == usuario.Codigo_Docente)
                        {
                            num = num + 1;
                            horas = horas + p.Tbl_DetallePlanEstudio.TotalHoras_DetallePlanEstudio;
                            creditos = creditos + p.Tbl_DetallePlanEstudio.TotalCreditos_DetallePlanEstudio;
                            <tr>
                                <td>@(num == 1 ? (p.Tbl_Docente.Nombres_Docente + ", " + p.Tbl_Docente.Apellidos_Docente) : "")</td>
                                <td>@num</td>
                                <td>@p.Tbl_DetallePlanEstudio.Asignatura_DetallePlanEstudio</td>
                                <td style="text-align:center">@p.Tbl_DetallePlanEstudio.TotalHoras_DetallePlanEstudio</td>
                                <td style="text-align:center">@p.Tbl_DetallePlanEstudio.TotalCreditos_DetallePlanEstudio</td>
                            </tr>
                        }
                    }
                    <tr>
                        <td></td>
                        <td></td>
                        <th bgcolor="#D9E2FF"><strong>Total</strong></th>
                        <th style="text-align:center" bgcolor="#D9E2FF"><strong>@horas</strong></th>
                        <th style="text-align:center" bgcolor="#D9E2FF"><strong>@creditos</strong></th>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

<!--Resumen general de PRUEBA DE ENTRADA-->
<br />
<div class="card-box ">
    <div class="row clearfix">
        <div class="col-md-12">
            <center><h2 class="text-primary">Estado Informe Prueba de Entrada</h2></center>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item active" aria-current="page">RESUMEN - CANTIDAD DE REGISTROS POR ESTADO</li>
                </ol>
            </nav>
        </div>
    </div>
    <div class="col-md-12">
        <div class="profile-view">
            <div class="row">
                <div class="col-md-6">
                    <div class="profile-info-left">
                        <ul class="personal-info">
                            <li>
                                <span class="title">No Generado:</span>
                                <span class="text">@cant_noguardado</span>
                                <span class="title">Guardado:</span>
                                <span class="text">@cant_guardado</span>
                                <span class="title">Enviado:</span>
                                <span class="text">@cant_enviado</span>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6">
                    <ul class="personal-info">
                        <li>
                            <span class="title">Aceptado:</span>
                            <span class="text">@cant_aceptado</span>
                            <span class="title">Rechazado:</span>
                            <span class="text">@cant_rechazado</span>
                            <span class="title">Total:</span>
                            <span class="text">@(cant_aceptado + cant_enviado + cant_guardado + cant_noguardado + cant_rechazado)</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-md-12">
            <div id="containerbb"></div>
            <script>
            var chart = Highcharts.chart('containerbb', {

                chart: {
                    type: 'column'
                },

                title: {
                    text: 'Información de Informe de Prueba de Entrada'
                },

                subtitle: {
                    text: 'ESTADO POR CADA CURSO'
                },

                legend: {
                    align: 'right',
                    verticalAlign: 'middle',
                    layout: 'vertical'
                },

                xAxis: {
                    categories: ['No Generado', 'Guardado', 'Enviado', 'Rechazado', 'Aceptado'],
                    labels: {
                        x: -10
                    }
                },

                yAxis: {
                    allowDecimals: false,
                    title: {
                        text: 'Nivel Alcanzado'
                    }
                },

                series: [@Html.Raw((String)estado_pruebaentrada)],

                responsive: {
                    rules: [{
                        condition: {
                            maxWidth: 500
                        },
                        chartOptions: {
                            legend: {
                                align: 'center',
                                verticalAlign: 'bottom',
                                layout: 'horizontal'
                            },
                            yAxis: {
                                labels: {
                                    align: 'left',
                                    x: 0,
                                    y: -5
                                },
                                title: {
                                    text: null
                                }
                            },
                            subtitle: {
                                text: null
                            },
                            credits: {
                                enabled: false
                            }
                        }
                    }]
                }
            });

            $('#small').click(function () {
                chart.setSize(400, 300);
            });

            $('#large').click(function () {
                chart.setSize(600, 300);
            });
            </script>
        </div>
    </div>
</div>

