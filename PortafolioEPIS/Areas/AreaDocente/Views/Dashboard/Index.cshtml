<!--ESTADO DE INFORME PRUEBA DE ENTRADA-->
@{
    int cant_noguardado = 0;
    int cant_guardado = 0;
    int cant_enviado = 0;
    int cant_rechazado = 0;
    int cant_aceptado = 0;
    string cursos_pruebaentrada = "";
    string estado_pruebaentrada = "";
}
@foreach (var item in detalle)
{
    if (item.Codigo_Docente == usuario.Tbl_Docente.Codigo_Docente)
    {
        string estadoNN = null;
        string cadena_estados = null;
        string color = null;
        string estadoss = null;
        string estadonombre = null;


        foreach (var q in prueba)
        {
            if (item.Codigo_DetalleCargaAcademica == q.Codigo_DetalleCargaAcademica)
            {
                estadoss = q.Estado_PruebaEntrada;
            }
        }
        if (estadoss == "G")
        {
            cant_guardado++;
            estadoNN = "0,100,0,0,0";
            estadonombre = "Guardado";
            color = "color: '#0597F2',";
        }
        if (estadoss == "E")
        {
            cant_enviado++;
            estadoNN = "0,0,100,0,0";
            estadonombre = "Enviado";
            color = "color: '#8416F2',";
        }
        if (estadoss == "R")
        {
            cant_rechazado++;
            estadoNN = "0,0,0,100,0";
            estadonombre = "Rechazado";
            color = "color: '#F20505',";
        }
        if (estadoss == "A")
        {
            cant_aceptado++;
            estadoNN = "0,0,0,0,100";
            estadonombre = "Aceptado";
            color = "color: '#04D976',";
        }
        if (estadoss == null)
        {
            cant_noguardado++;
            estadoNN = "100,0,0,0,0";
            estadonombre = "No Generado";
            color = "color: '#8C8C8C',";
        }

        //algoritmo para generar el grafico
        cursos_pruebaentrada = cursos_pruebaentrada + "'" + item.Tbl_DetallePlanEstudio.Asignatura_DetallePlanEstudio + "', ";
        cadena_estados = cadena_estados + "{name: '" + item.Tbl_DetallePlanEstudio.Asignatura_DetallePlanEstudio + "'," + color + "data: [" + estadoNN + "]},";
        estado_pruebaentrada = estado_pruebaentrada + cadena_estados;
    }
}

