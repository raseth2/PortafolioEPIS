                                                <th>Créditos</th>
                                                <th>Ciclo</th>
                                                <th class="text-center">Estado</th>
                                                <th class="text-right">Accción</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{ int num0 = 0;}
                                            @foreach (var p in Model)
                                            {
                                                num0 = num0 + 1;

                                                if (p.Codigo_PlanEstudio.Equals(Convert.ToInt32(ViewBag.id)))
                                                {
                                                <tr>
                                                    <td>@num0</td>
                                                    <td><a href="~/DetallePlanEstudio/Agregar1/@p.Codigo_DetallePlanEstudio">@p.CodigoCurso_DetallePlanEstudio</td>
